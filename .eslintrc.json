{
  "extends": "@antfu",
  "rules": {
    "@typescript-eslint/indent": [
      "error",
      4
    ],
    "vue/html-indent": [
      "error",
      4
    ],
    "vue/max-attributes-per-line": [
      "error",
      {
        "singleline": {
          "max": 1
        },
        "multiline": {
          "max": 1
        }
      }
    ]
  }
}
